package Aula44Exercicio01;

public class Piramide extends Figura3D implements DimensaoSuperficial, DimensaoVolumetrica {


	@Override
	public double calcularArea(double lado1, double lado2) {
		// TODO Auto-generated method stub
		return 0;
	}
	
	@Override
	public double calcularVolume(double altura, double largura, double comprimento) {
		return altura * largura * comprimento;
	}

}
