		System.out.println(Cafe.getContador());
		System.out.println(Cafe.getContador());
		System.out.println(Cafe.getContador());
		
		Embalagem embalagemMelita = new Embalagem ();
		embalagemMelita.fabricante = "Astromex";
		embalagemMelita.formato = "caixa";
		
		TipoGrao graoMelita = new TipoGrao ();
